issn = {1996-1073},
journal = {Energies},
keywords = {boost converters,dc,dc power converters,gan switch,resonant power conversion,zcs,zero-current switching,zero-voltage switching,zvs},
mendeley-groups = {XC/DC Converters/DC/DC Converters/NonIsolated},
month = {dec},
