doi = {10.1109/TPEL.2015.2449322},
file = {:C$\backslash$:/Users/Furkan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Konjedic et al. - 2016 - DCM-based zero-voltage switching control of a bidirectional DC-DC converter with variable switching frequency.pdf:pdf},
issn = {08858993},
journal = {IEEE Transactions on Power Electronics},
keywords = {Bidirectional DC-DC converter,Discontinuous conduction mode (DCM),Resonant switching transitions,Synchronous rectification,Zero-voltage switching (ZVS)},
number = {4},
