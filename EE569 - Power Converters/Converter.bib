file = {:C$\backslash$:/Users/Furkan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rodriguez et al. - 2018 - Synchronous Boost Converter With High Efficiency at Light Load Using QSW-ZVS and SiC mosfets.pdf:pdf},
issn = {02780046},
